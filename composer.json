{
    "name": "deegitalbe/pipedrive-client",
    "description": "Basic versioned php package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Deegitalbe\\PipedriveClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deegitalbe\\PipedriveClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deegitalbe\\PipedriveClient\\Providers\\PipedriveClientServiceProvider"
            ]
        }
    }
}
